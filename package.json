    "ts-loader": "^7.0.2",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "codecov": "^3.6.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma": "^5.0.2",
